{
  "name": "updb-app",
  "private": true,
  "version": "0.0.0",
  "imports": {
    "@/*": "src/*.ts"
  },
  "scripts": {
    "dev": "node server-dev",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.ts --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "chrono-node": "^2.3.8",
    "compression": "^1.7.4",
    "html-entities": "^2.3.3",
    "luxon": "^2.3.1",
    "qs": "^6.10.3",
    "tabulator-tables": "^5.1.7",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
    "@vitejs/plugin-vue": "^2.2.0",
    "@volar/vue-language-service-types": "^0.33.7",
    "express": "^4.18.1",
    "sass": "^1.49.9",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.3",
    "vite": "^2.8.0",
    "vite-svg-loader": "^3.2.0",
    "vue-tsc": "^0.29.8"
  }
}
